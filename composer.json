{
  "name": "adamus/laravel-iam-client",
  "version": "1.1.0",
  "description": "Laravel package for integrating with Adamus IAM service for centralized authentication",
  "type": "library",
  "keywords": [
    "laravel",
    "authentication",
    "iam",
    "sso",
    "adamus"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Adamus",
      "email": "stanleyotabil10@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/auth": "^11.0|^12.0",
    "illuminate/http": "^11.0|^12.0",
    "illuminate/routing": "^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Adamus\\LaravelIamClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Adamus\\LaravelIamClient\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Adamus\\LaravelIamClient\\IAMClientServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
